<h1>Listing Votes</h1>

<table>
  <thead>
    <tr>
      <th>Hold date</th>
      <th>Presenter</th>
      <th>Score</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @votes.each do |vote| %>
      <tr>
        <td><%= vote.hold_date %></td>
        <td><%= vote.presenter %></td>
        <td><%= vote.score %></td>
        <td><%= vote.comment %></td>
        <td><%= link_to 'Show', vote %></td>
        <td><%= link_to 'Edit', edit_vote_path(vote) %></td>
        <td><%= link_to 'Destroy', vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote', new_vote_path %>
